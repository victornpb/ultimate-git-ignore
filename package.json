{
  "name": "ultimate-gitignore",
  "version": "1.0.0",
  "description": "The ultimate gitignore file for JavaScript projects and libraries",
  "bin": "download.sh",
  "scripts": {
    "start": "npm run watch",
    "build": "node index.js",
    "watch": "nodemon --ext txt,js --exec \"npm run build\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victornpb/ultimate-gitignore.git"
  },
  "keywords": [
    "gitignore",
    "git",
    "ignore",
    "javascript",
    "default",
    "boilerplate",
    "scaffolding",
    "template",
    "repository",
    "node",
    "vue",
    "react",
    "rollup",
    "webpack",
    "babel"
  ],
  "author": "victornpb",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/victornpb/ultimate-gitignore/issues"
  },
  "homepage": "https://github.com/victornpb/ultimate-gitignore#readme",
  "devDependencies": {
    "asciitable.js": "^1.0.3",
    "nodemon": "^2.0.15",
    "tiny-dedent": "^1.0.2"
  },
  "engines": {
    "node": ">15.0.0"
  }
}
